rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write, list: if false;
    }
   
    match /profiles/{userId} {
      allow read: if true;
    }

    match /posts/{postId} {
      allow read: if true;
    }

    match /profiles/{userId}/posts/{postId}{
      allow read: if true;
    }
  }
}